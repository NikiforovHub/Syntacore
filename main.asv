read;


counter = zeros(1, size(data,2));

for k = 1:(2^(size(data,1)) - 1)
    binStr = dec2bin(k,size(data,1));
    binStr = split(binStr, '');
    binStr = str2num(cell2mat(binStr));
    binStr = binStr';
    
    vec = binStr*data;
    vec = rem(vec, 2);
    
    
    w = weight(vec);
    if w > 0
        counter(w) = counter(w) + 1;
    else
        zerocounter
    end
end


bar(0:(size(data,2)-1), counter)

